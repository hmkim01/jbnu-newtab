{
    "name": "JBNU 새탭",
    "description": "성실한 전북대 학생을 위한 새탭 extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "host_permissions": [
      "*://*.jbnu.ac.kr/*"
    ],
    "chrome_url_overrides": {
        "newtab": "./src/index.html"
      },
      "icons": {
        "16": "./assets/images/심볼_01.png",
        "32": "./assets/images/심볼_01.png",
        "48": "./assets/images/심볼_01.png",
        "128": "./assets/images/심볼_01.png"
      }
}